<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Mt Test Suite">
	<groups>
		<run>
			<include name="smoke"></include>
			<include name="sanity"></include>
		</run>
	</groups>
	
	
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="browsergroup1" value="chrome"></parameter>
	<parameter name="browsergroup2" value="IE"></parameter>
	
	
	<listeners>
		<listener class-name="listener.MyCustomListener"></listener>
	</listeners>
	<suite-files>
		<suite-file path="suitea.xml"/>
		<suite-file path="suiteb.xml"/>
		<suite-file path="suitec.xml"/>
		</suite-files>        
</suite> <!-- Suite -->
